 RowBox[{
  RowBox[{"choleskyDecomposition", "[", 
   RowBox[{"mat_", "?", "SymmetricMatrixQ"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"n", "=", 
       RowBox[{"Length", "[", "mat", "]"}]}], ",", 
      RowBox[{"mt", "=", "mat"}], ",", "v", ",", "\[Alpha]", ",", "L"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"L", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"v", "=", 
         RowBox[{"mt", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"2", ";;", 
             RowBox[{"n", "-", "j", "+", "1"}]}], ",", "1"}], "]"}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\<v \>\"", " ", "\"\<=\>\"", " ", 
          RowBox[{"MatrixForm", " ", "@", " ", "v"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"\[Alpha]", "=", 
         RowBox[{"mt", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"L", "[", 
          RowBox[{"[", 
           RowBox[{"j", ",", "j"}], "]"}], "]"}], "=", 
         RowBox[{"Sqrt", "[", "\[Alpha]", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"L", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{
             RowBox[{"j", "+", "1"}], ";;", "n"}], ",", "j"}], "]"}], "]"}], 
         "=", 
         RowBox[{"v", "/", 
          RowBox[{"L", "[", 
           RowBox[{"[", 
            RowBox[{"j", ",", "j"}], "]"}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"v", "=", 
         RowBox[{
          RowBox[{"{", "v", "}"}], "\[Transpose]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"mt", "=", 
